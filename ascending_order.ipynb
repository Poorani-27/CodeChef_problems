    "def ascending_order(arr):\n",
    "    left = 0\n",
    "    right = len(arr)-1\n",
    "    while left  < right :\n",
    "        temp = arr[left]\n",
    "        arr[left] = arr[right]\n",
    "        arr[right] = temp\n",
    "        left +=1\n",
    "        right -=1\n",
    "        \n",
    "    return arr\n",
    "\n",
    "arr=[10,5,4,12,3]\n",
    "print(ascending_order(arr))\n",
    "    "
   ]
  }
