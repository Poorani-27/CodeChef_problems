    "def print_alpha(string):\n",
    "    new_string = \"\"\n",
    "    for s in string:\n",
    "        if 'a' <= s <= 'z' or 'A' <= s <= 'Z' or s==\" \":\n",
    "            new_string += s\n",
    "    return new_string\n",
    "\n",
